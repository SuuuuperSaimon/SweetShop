{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}
    <h1>{{ title }}</h1>
    <div class="list-group">
        {% for category in categories  %}
            <a href="{{ path('district_show', {id: category.id}) }}">{{ category.categoryName }}</a>
        {% endfor %}
    </div>
    <a href="{{ path('category_new') }}">Create new</a>
    <a href="{{ path('category') }}">Back to List</a>
{% endblock %}
